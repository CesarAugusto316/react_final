{
  // "typescript.disableAutomaticTypeAcquisition": true,
  // "js/ts.implicitProjectConfig.checkJs": true,
  // "python.languageServer": "Pylance",
  "[javascriptreact][javascript][typescript][typescriptreact][json]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[html][css][scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.validate": [
    "javascript",
    "typescript"
    // "html",
    // "css",
  ],
  "eslint.format.enable": true,
  "eslint.run": "onSave",
  "emmet.includeLanguages": {
    "javascript": "javascriptreact", //intellisense for jsxElements
    "typescript": "typescriptreact"
  },
  "zenMode.hideLineNumbers": false,
  "editor.glyphMargin": false,
  "editor.renderWhitespace": "boundary",
  "editor.fontFamily": "Fira Code", //JetBrains Mono, Fira Code, "'Hack FC Ligatured CCG', 'Hack', Menlo, Monaco, 'Courier New', monospace"
  "editor.fontSize": 15,
  "editor.letterSpacing": -0.2,
  "editor.lineHeight": 1.25,
  "editor.fontLigatures": true,
  "editor.fontWeight": "430",
  "editor.cursorStyle": "block",
  "editor.suggestFontSize": 14,
  "editor.tabSize": 2,
  "editor.useTabStops": true,
  "editor.folding": true,
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.shellIntegration.enabled": true,
  "debug.console.fontSize": 14.5,
  "debug.console.lineHeight": 14,
  "[Log]": {
    "editor.fontSize": 13.5
  },
  "editor.inlayHints.fontSize": 14,
  "workbench.tree.indent": 11,
  "workbench.colorTheme": "GapStyle VS",
  "workbench.iconTheme": "vscode-icons",
  "editor.guides.highlightActiveBracketPair": true,
  "editor.guides.bracketPairs": true,
  "editor.bracketPairColorization.enabled": true,
  "customWindowZoom.zoomLevelChangeValue": 0.15,
  "editor.guides.highlightActiveIndentation": "always",
  "editor.minimap.enabled": false,
  "editor.semanticTokenColorCustomizations.enabled": true,
  "colorize.languages": [
    "json",
    "css",
    "html",
    "sass",
    "scss",
    "javascript",
    "typescript"
  ],
  "colorize.colorized_variables": ["SASS", "CSS"],
  "colorize.include": [
    "**/*.css",
    "**/*.scss",
    "**/*.sass",
    "**/*.json",
    "**/*.js",
    "**/*.jsx",
    "**/*.ts",
    "**/*.tsx",
    "**/*.md",
    "**/*.html"
  ],
  "editor.semanticTokenColorCustomizations": {
    "[GapStyle VS]": {
      "enabled": true,
      "rules": {
        "property": {
          "foreground": "#9876AA", // #9876AA
          "fontStyle": "italic"
        },
        "function.defaultLibrary": {
          "foreground": "#3fc1c5", //66D9EF, 46d6db
          "fontStyle": "italic"
        }
      }
    }
  },
  "editor.tokenColorCustomizations": {
    "[Solarized Custom Light Italic][Solarized Custom Light]": {
      "textMateRules": [
        {
          "scope": ["support.class.component.js.jsx"],
          "settings": {
            "foreground": "#728302", //728302, 268bd2
            "fontStyle": "italic bold"
          }
        },
        {
          "scope": [
            "storage.type.function.arrow",
            "entity.name.function",
            "variable.parameter"
          ],
          "settings": {
            "fontStyle": "bold"
          }
        }
      ]
    },
    "[GapStyle VS]": {
      "textMateRules": [
        {
          "scope": [
            "keyword.operator",
            "keyword.operator.comparison",
            "keyword.operator.increment",
            "keyword.operator.relational",
            "keyword.operator.arithmetic",
            "keyword.operator.decrement",
            "storage.type.function.arrow",
            "punctuation.separator.key-value",
            "punctuation.accessor",
            "keyword.operator.ternary",
            "keyword.operator.logical",
            "punctuation.destructuring.js"
          ],
          "settings": {
            "fontStyle": "bold"
          }
        },
        {
          "scope": [
            "support.class.component.js",
            "support.class.component.tsx"
          ],
          "settings": {
            "fontStyle": "bold",
            "foreground": "#8754fd" // 6d54fd
          }
        },
        {
          "scope": [
            "support.type.primitive.tsx",
            "support.type.primitive.ts",
            "support.type.builtin.tsx",
            "support.type.builtin.ts"
          ],
          "settings": {
            "fontStyle": ""
          }
        },
        {
          "scope": "variable.other.property.js",
          "settings": {
            "foreground": "#9876AA",
            "fontStyle": "italic"
          }
        },
        {
          "scope": [
            "keyword.control.export",
            "keyword.operator.definiteassignment.tsx",
            "keyword.operator.definiteassignment.ts",
            // "keyword.control.import",
            // "keyword.control.from",
            "keyword.control.flow",
            "keyword.control.conditional",
            "keyword.operator.optional.ts",
            "keyword.operator.optional.tsx",
            "keyword.operator.new"
          ],
          "settings": {
            "fontStyle": "italic bold"
          }
        },
        {
          "scope": ["text.html.derivative", "meta.jsx.children"],
          "settings": {
            "foreground": "#8692a3"
          }
        },
        {
          "scope": [
            "constant.language.boolean.true.tsx",
            "constant.language.boolean.true.ts",
            "constant.language.boolean.true.js"
          ],
          "settings": {
            "foreground": "#0ee26e" // 18f57c
          }
        },
        {
          "scope": [
            "constant.language.boolean.false.tsx",
            "constant.language.boolean.false.ts",
            "constant.language.boolean.false.js"
          ],
          "settings": {
            "foreground": "#f24f8b" //#fd4d8e
          }
        },
        {
          "scope": [
            "storage.modifier.async",
            "keyword.control.switch",
            "keyword.control.default"
          ],
          "settings": {
            "fontStyle": "italic"
          }
        },
        {
          "scope": [
            "comment.block.documentation",
            "comment.block.documentation.ts",
            "comment.block.documentation.js",
            "entity.name.type.instance.jsdoc"
          ],
          "settings": {
            "foreground": "#6e9e62d5"
          }
        },
        {
          "scope": ["comment.line.double-slash", "comment.block"],
          "settings": {
            "foreground": "#636262"
          }
        },
        {
          "scope": "punctuation.definition.entity.css",
          "settings": {
            "foreground": "#6D9CBE"
          }
        },
        {
          "scope": [
            "variable.other.readwrite.js.jsx",
            "variable.other.readwrite.ts.tsx"
          ],
          "settings": {
            "foreground": "#FD971F"
          }
        }
      ]
    }
  },
  "workbench.colorCustomizations": {
    "[Solarized Custom Light Italic]": {
      "editorGutter.background": "#f3ead4"
    },
    "[GapStyle VS]": {
      "editor.background": "#23272e", // 192c2c, 1a2f2f, 182c2c, #282C34, 23272e
      "editorGutter.background": "#282C34", // 253c42, 282C34
      "editorLineNumber.foreground": "#91a4b6",
      "editorLineNumber.activeForeground": "#2b51fd",
      "editorBracketHighlight.foreground1": "#578dff",
      "editorBracketHighlight.foreground2": "#d8ade4", // 14ff14
      "editorBracketHighlight.foreground3": "#ff9f51",
      "editorBracketHighlight.foreground4": "#dee9fd",
      "editorBracketHighlight.foreground5": "#fc5ed4",
      "editorBracketHighlight.foreground6": "#31d6e9",
      "editorBracketHighlight.unexpectedBracket.foreground": "#ff8084"
    }
  },
  "security.workspace.trust.untrustedFiles": "open",
  "livePreview.autoRefreshPreview": "On Changes to Saved Files",
  "livePreview.notifyOnOpenLooseFile": false,
  "files.autoSaveDelay": 4500,
  "notebook.lineNumbers": "on",
  "notebook.markup.fontSize": 14,
  "git.terminalAuthentication": false,
  "editor.wordWrap": "off",
  "diffEditor.wordWrap": "off",
  "editor.guides.indentation": true,
  "editor.inlayHints.enabled": "on",
  "window.menuBarVisibility": "toggle",
  "vscode-edge-devtools.mirrorEdits": true,
  "vscode-edge-devtools.fallbackRevision": "@a41122be052fe9616f0def5fe6842fa942930d46",
  "vscode-edge-devtools.themes": "Dark",
  "vscode-edge-devtools.welcome": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.currentLine.enabled": false,
  "workbench.tree.renderIndentGuides": "always",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "telemetry.telemetryLevel": "off",
  "thunder-client.codeSnippetLanguage": "js-axios",
  "timeline.excludeSources": ["timeline.localHistory"],
  "window.zoomLevel": 0,
  "thunder-client.htmlView": "Preview"
}
